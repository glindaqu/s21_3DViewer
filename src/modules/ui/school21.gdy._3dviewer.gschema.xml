<schemalist>
  <schema id="school21.gdy._3dviewer" path="/src/modules/ui/school21/gdy/3dviewer/">
    <!-- Настройки проекции -->
    <key name="projection-type" type="s">
      <choices>
        <choice value ="Parallel"/>
        <choice value ="Central"/>
      </choices>
      <default>'Parallel'</default>
      <summary>Type of projection</summary>
      <description>The type of projection to use (parallel or central).</description>
    </key>

    <!-- Настройки точек -->
    <key name="point-type" type="s">
      <choices>
        <choice value ="None"/>
        <choice value ="Circle"/>
        <choice value ="Square"/>
      </choices>
      <default>'None'</default>
      <summary>Type of points</summary>
      <description>The type of points to display (none, circle or square).</description>
    </key>

    <!-- Настройки точек -->
    <key name="point-size" type="i">
      <default>1</default>
      <summary>Point size</summary>
      <description>The size of the points.</description>
    </key>

    <!-- Настройки цвета точек -->
    <key name="point-color" type="(iiii)">
      <default>(0,0,0,1)</default>
      <summary>Points color</summary>
      <description>The color of the points.</description>
    </key>

    <!-- Настройки ребер -->
    <key name="edge-type" type="s">
      <choices>
        <choice value ="Solid"/>
        <choice value ="Dashed"/>
      </choices>
      <default>'Solid'</default>
      <summary>Type of edges</summary>
      <description>The type of edges to display (solid or dashed).</description>
    </key>

    <!-- Настройки толщины линий -->
    <key name="edge-thickness" type="i">
      <default>1</default>
      <summary>Edge thickness</summary>
      <description>The thickness of the edges.</description>
    </key>

    <!-- Настройки цвета ребер -->
    <key name="edge-color" type="(iiii)">
      <default>(0,0,0,1)</default>
      <summary>Edges color</summary>
      <description>The color of the edges.</description>
    </key>

    <!-- Настройки цвета фона -->
    <key name="background-color" type="(iiii)">
      <default>(0,0,0,1)</default>
      <summary>Background color</summary>
      <description>The color of the background.</description>
    </key>
  </schema>
</schemalist>